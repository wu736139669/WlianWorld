@implementation ViewController

- (void)viewDidLoad {
    [super viewDidLoad];
    // Do any additional setup after loading the view, typically from a nib.
    self.title = @"111";
    
    
    BaseEntity* baseVM = [BaseViewModel requireBase];

    RACSignal* singnal = [BaseWLService requireWithProperty:baseVM];
    [singnal subscribeNext:^(id x) {
        DLog(@"x%@",x);
    }];
    [singnal subscribeError:^(NSError *error) {
        DLog(@"%@",error);
    }];
    
}

